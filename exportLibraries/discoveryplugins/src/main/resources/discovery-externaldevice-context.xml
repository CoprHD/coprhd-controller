<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <!-- Discovery drivers -->
    <bean id="scaleioDriver" class="com.emc.storageos.driver.scaleio.ScaleioStorageDriver">
    </bean>
    <bean id="cloudArrayDriver" class="com.emc.storageos.driver.cloudarray.CloudArrayStorageDriver">
    </bean>

    <bean id="externaldevice" class="com.emc.storageos.volumecontroller.impl.plugins.ExternalDeviceCommunicationInterface">
        <property name="drivers">
            <map>
                <entry key="scaleio" value-ref="scaleioDriver"/>
                <entry key="cloudarray" value-ref="cloudArrayDriver"/>
            </map>
        </property>
    </bean>


</beans>
