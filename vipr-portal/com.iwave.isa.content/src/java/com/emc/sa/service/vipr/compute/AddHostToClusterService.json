{
  baseKey:"AddHostToCluster.Compute",
  serviceId:"AddHostToCluster",
  fields:{
    project:{
      type:"assetType.vipr.project",
      lockable: true
    },
    cluster:{
      type:"assetType.vipr.cluster",
      lockable: true
    },
    virtualArray:{
      type:"assetType.vipr.blockVirtualArray",
      lockable: true
    },
    computeVirtualPool: {
      type:"assetType.vipr.computeVirtualPool"
    },
    computeImage:{
      type:"assetType.vipr.computeImage",
      lockable: true
    }, 
    hosts:{
      type:"table",
      items:{
          fqdns:{ type:"text",
            validation:{
              max:255
            }          
          },
          ips:{ type:"text",
            validation:{
            max:255
          }          
        }
      }
    },
    netmask:{
      type:"text",
       validation:{
        max:255
      }
    },
    gateway:{
      type:"text",
      validation:{
        max:255
      }
    },
    managementNetwork:{
      type:"number",
      validation:{
        min:1,
        max:4093
      }
    },
    ntpServer:{
      type:"text",
      required:false,
      validation:{
        max:255        
      }
    },
    dnsServers:{
      type:"text",
      required:false,
        validation:{
        max:255        
      }
    },
    hostPassword:{
      type:"password",
      required:true,
        validation:{
        max:128        
      }
    },
    virtualPool:{
      type:"assetType.vipr.blockVirtualPool",
      lockable: true,
      group:1
    },
    size:{
      type:"storageSize",
      validation:{
        min:1
      }
    },
    vcenter:{
      type:"assetType.vipr.vcentersForCluster",
      lockable: true,
      required:false
    },
    datacenter:{
      type:"assetType.vipr.datacentersForCluster",
      lockable: true,
      required:false
    }
  }  
}
