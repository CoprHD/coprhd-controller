{
  baseKey:"CreateBlockSnapshot.Block",
  serviceId:"CreateBlockSnapshot",
  fields:{
    project:{
      type:"assetType.vipr.project",
      lockable: true
    },

    storageType:{
      type:"assetType.vipr.blockVolumeOrConsistencyType",
      lockable:true,
      initialValue:"volume"
    },
    
    volumes:{
      type:"assetType.vipr.snapshotBlockVolume"
    },
    
    type: {
      type: "assetType.vipr.blockSnapshotType",
      required: "false",
      lockable: true
    },
        
    readOnly: {
      type: "boolean",
      initialValue: "false",
      required: "false",
      lockable: true
    },
    
    name:{
      type:"text",
      validation:{
        min:2,
        max:128
      }
    },
    
    advanced:{
      type:"group",
      lockable: true,
      collapsed:true,
      items:{
        linkedSnapshotName:{
          type:"text",
          required: "false",
          validation:{
            min:2,
            max:128
          }
        },
        linkedSnapshotCount:{
          type:"number",
          required: "false",
          initialValue: "1",
          validation:{
              min:1
          }
        }
      }
    }
  }
}
