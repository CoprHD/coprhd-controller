{
  baseKey:"FailoverBlockVolume.Block",
  serviceId:"FailoverBlockVolume",
  destructive: true,
  fields: {
    project:{
      type:"assetType.vipr.project",
      lockable: true
    },
    
    storageType:{
      type:"assetType.vipr.blockVolumeOrConsistencyType",
      lockable:true,
      initialValue:"volume"
    },
	
    volumes:{
      type:"assetType.vipr.protectedBlockVolume"
    },

    failoverTarget:{
      type: "assetType.vipr.failoverTarget"
    },
    
    advanced:{
      type:"group",
      lockable: true,
      collapsed:true,
      items:{
        imageToAccess:{
          type: "assetType.vipr.imageToAccess",
          required: "false",
          initialValue: "latest",
          lockable: true
        },     
        pointInTime:{
          type: "dateTime",
          required: "false",
          lockable: true
        },
        directAccess: {
          type: "boolean",
          initialValue: "false",
          required: "false",
          lockable: true
        }
      }  
    }    

  }
}
