_get_monorail_props() {
    _get_props network_ipaddr_service network_vip_service
}

_genconfig_exec _gen_template_cluster    /etc/monorail/config.json  $(_get_monorail_props)
_genconfig_exec _gen_template_cluster    /etc/monorail/hosts $(_get_monorail_props)
